@font-face
  font-family: Inconsolata Mono
  font-style: normal
  font-weight: normal
  src: url("Inconsolata.otf")

.normal-mono
  font-family: "Courier New", Courier, monospace

tt, pre, .mono
  /* Kindle Keyboard has KF8 but no font support, fallback to default mono */
